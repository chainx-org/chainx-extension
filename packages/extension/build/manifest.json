{
  "description": "ChainX test chrome extention",
  "name": "ChainX extension",
  "short_name": "ChainX",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "ChainX",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/dog.png",
    "32": "images/dog.png",
    "48": "images/dog.png",
    "64": "images/dog.png",
    "128": "images/dog.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'wasm-eval'; object-src 'self'",
  "version": "1.0.0"
}